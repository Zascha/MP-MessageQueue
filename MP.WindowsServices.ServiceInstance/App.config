<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="listenerSection" type="MP.WindowsServices.Common.ConfigurationHelper.AppConfiguration, MP.WindowsServices.Common" />
  </configSections>
  <listenerSection>
    <fileNameTemplate template="[a-zA-Z0-1]+_[0-9]{3}.[a-zA-Z]{3,4}" />
    <fileFilters filters="jpg, jpeg, png, tiff" />
    <observableFolders>
      <folder path="D:\ObservableFolder" />
    </observableFolders>
  </listenerSection>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.8.1.0" newVersion="4.8.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>